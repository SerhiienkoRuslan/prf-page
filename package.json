  "devDependencies": {
    "@babel/eslint-plugin": "^7.19.1",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@changesets/cli": "^2.26.0",
    "@types/react-modal-image": "^2.6.3",
